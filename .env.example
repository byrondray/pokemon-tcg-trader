# Turso Database
TURSO_DATABASE_URL=libsql://your-database-name.turso.io
TURSO_AUTH_TOKEN=your_turso_auth_token_here

# Kinde Auth
KINDE_DOMAIN=https://your-domain.kinde.com
KINDE_CLIENT_ID=your_kinde_client_id_here
KINDE_CLIENT_SECRET=your_kinde_client_secret_here
KINDE_REDIRECT_URI=http://localhost:5173/api/auth/kinde_callback
KINDE_LOGOUT_REDIRECT_URI=http://localhost:5173
KINDE_SCOPE=profile email offline openid

# AWS S3
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name

# Cloudinary
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Encryption
ENCRYPTION_KEY=your_32_character_encryption_key_here

# WebSocket
SOCKET_IO_SERVER_URL=http://localhost:5173
